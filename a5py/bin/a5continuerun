#!/usr/bin/env python

import a5py.postprocessing.endstate2markers as endstate2markers

import sys

if len(sys.argv) < 3:
   print("Not enough arguments. Provide filename and markertype (fo, gc, ml).")

fn = sys.argv[1]
markertype = sys.argv[2]

endstate2markers.convert(fn,markertype)
print("Endstate converted to markers.")