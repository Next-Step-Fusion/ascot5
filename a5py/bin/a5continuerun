#!/usr/bin/env python3

import a5py.postprocessing.state2markers as state2markers

import sys

if len(sys.argv) < 3:
   print("Not enough arguments. Provide filename and markertype (fo, gc, ml).")

fn = sys.argv[1]
markertype = sys.argv[2]

state2markers.convert(fn,markertype,"endstate",256)
print("Endstate converted to markers.")
